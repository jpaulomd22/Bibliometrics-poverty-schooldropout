@ARTICLE{Jonas2016,
author={Jonas, K. and Crutzen, R. and Van Den Borne, B. and Sewpaul, R. and Reddy, P.},
title={Teenage pregnancy rates and associations with other health risk behaviours: A three-wave cross-sectional study among South African school-going adolescents},
