@ARTICLE{Ofori-Boateng2020236,
author={Ofori-Boateng, K. and Adams, S. and Ohemeng, W.},
title={Coping Strategies of the Urban Poor: A Case Study From Ghana},
journal={Poverty and Public Policy},
year={2020},
volume={12},
number={3},
pages={236-254},
doi={10.1002/pop4.286},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85090547342&doi=10.1002%2fpop4.286&partnerID=40&md5=a8ca58b965924f4deb661da02c7c6065},
affiliation={Accounting and Finance, Ghana Institute of Management and Public Administration, Achimota, Accra, Ghana; Department of Economics, Ghana Institute of Management and Public Administration, Achimota, Accra, Ghana},
abstract={The study examines the profile and livelihood strategies of the urban poor. Utilizing the primary data from three selected poverty prone areas in Accra, the study traces the migratory history of 216 poor urbanized youth and solicits how they cope with an urban livelihood. The findings show that respondents did not receive any formal social protection and informal support from family, and social clubs/associations were also minimal. Coping strategies adopted by these respondents included: petty trading (36.2 percent), street “kayayee” (kayaye is a local name given to head porters/truck pushers for a fee. It a common feature at the Central Business District of Accra; 25.5 percent), street hawking (12.8 percent), and truck pushing (10.6 percent). Some also get involved in illegal and immoral activities such as theft (in the case of males) and prostitution (in the case of the females). It is recommended that since most of them are school dropouts and above the age of availing themselves to the new free senior high school education being implemented by the government, both formal and informal skills (apprenticeship), and vocational training should be rigorously and diligently pursued to lift them out of poverty. © 2020 Policy Studies Organization},
author_keywords={coping mechanism;  Ghana;  poverty prone areas urbanization;  urban poor},
