@ARTICLE{Prakash2019,
author={Prakash, R. and Beattie, T.S. and Javalkar, P. and Bhattacharjee, P. and Ramanaik, S. and Thalinja, R. and Murthy, S. and Davey, C. and Gafos, M. and Blanchard, J. and Watts, C. and Collumbien, M. and Moses, S. and Heise, L. and Isac, S.},
title={The Samata intervention to increase secondary school completion and reduce child marriage among adolescent girls: Results from a clusterrandomised control trial in India},
