@ARTICLE{Reynolds20012339,
author={Reynolds, A.J. and Temple, J.A. and Robertson, D.L. and Mann, E.A.},
title={Long-term Effects of an Early Childhood Intervention on Educational Achievement and Juvenile Arrest},
