@article{ WOS:000207805600007,
Author = {Mudege, Netsayi N. and Zulu, Eliya M. and Izugbara, Chimaraoke},
Title = {{How Insecurity Impacts on School Attendance and School Dropout among
   Urban Slum Children in Nairobi}},
Journal = {{INTERNATIONAL JOURNAL OF CONFLICT AND VIOLENCE}},
Year = {{2008}},
Volume = {{2}},
Number = {{1}},
Pages = {{98-112}},
Abstract = {{This paper discusses how perceptions of personal security can impact on
   school enrolment and attendance. It mainly focuses on threats of
   physical harm, crime, and community and domestic violence. These
   security fears can include insecurity that children suffer from as they
   go to school, maybe through the use of unsafe routes; insecurity that
   children feel at school; and the insecurity they suffer from in their
   homes. Although poverty can be a source and/or an indicator of
   insecurity, this paper does not focus solely on poverty as it is well
   covered elsewhere in the literature. The paper relies on qualitative
   data collected in Korogocho and Viwandani slum areas in Nairobi, Kenya
   between October and November 2004. The paper analyses data from
   individual interviews and focus group interviews and focuses on the
   narrative of slum dwellers on how insecurity impacts on educational
   attainment. The conclusion in this paper is that insecure neighbourhoods
   may have a negative impact on schooling. As a result policies that
   address insecurity in slum neighbourhoods can also improve school
   attendance and performance.}},
