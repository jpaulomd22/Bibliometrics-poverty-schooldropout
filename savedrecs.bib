@article{ WOS:000402658600005,
Author = {Li, Qiang and An, Lian and Tan, Jingzhuo and Zheng, Changde and Yang,
   Mengke and Gong, Chiyuan},
Title = {{School Dropout, Ethnicity and Religion: Evidence from Western Rural
   China}},
Journal = {{CHINESE ECONOMY}},
Year = {{2017}},
Volume = {{50}},
Number = {{3, SI}},
Pages = {{193-204}},
Abstract = {{This article studies the determinants of school dropout decisions for
   western rural China by using the original and unique survey of 6,000
   families in western rural China. Special focus is given to the impact of
   religion on dropout decisions. We find that children from religious
   families are more likely to drop out of school than other children,
   especially for those practicing Tibetan Buddhism. In addition, we find
   that increase in a child's age, being a boy, and being part of an ethnic
   minority will increase the dropout probability while an increase in the
   mother's education will decrease the dropout probability. Family income
   and migration status do not have significant impact on the dropout
   decision in western rural China.}},
